{
"name": "BayesianBinomialTest",
	"autorun": true,
    "version": "2.00",
    "jaspResults": true,
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
		{
			"format": "",
			"name": "testValue",
			"type": "Number",
			"value": 0.5,
			"max": 1,
			"min": 0
		},
		{
			"format": "",
			"name": "priorA",
			"type": "Number",
			"value": 1,
			"min": 0.1,
			"max": 9999
		},
		{
			"format": "",
			"name": "priorB",
			"type": "Number",
			"value": 1,
			"min": 0.1,
			"max": 9999
		},
		{
			"name": "hypothesis",
			"options": [ "notEqualToTestValue", "greaterThanTestValue", "lessThanTestValue" ],
			"default": "notEqualToTestValue",
			"type": "List"
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": true,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysis",
			"type": "Boolean"
		}
	]
}
