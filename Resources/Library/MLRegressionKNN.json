{
	"name": "MLRegressionKNN",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "target",
			"type": "Variable"
		},
		{
			"name": "predictors",
			"type": "Variables"
		},
		{
			"name": "indicator",
			"type": "Variable"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				},
				{
					"name": "isNuisance",
					"type": "Boolean"
				}
			],
			"type": "Table"
		},
		{
			"name": "percentageTrainingData",
			"type": "List",
			"options": [ "auto", "manual" ],
			"default": "auto"
		},
		{
			"name": "trainingDataManual",
            "type": "Integer",
            "default": 80,
            "min": 1,
            "max": 99
		},
		{
			"name": "noOfNearestNeighbours",
			"type": "List",
			"options": [ "auto", "manual", "optimized" ],
			"default": "auto"
		},
		{
			"name": "nearestNeighboursCount",
            "type": "Integer",
            "default": 1
		},
		{
			"name": "optimizedFrom",
            "type": "Integer",
            "default": 1
		},
		{
			"name": "optimizedTo",
            "type": "Integer",
            "default": 2
		},
		{
			"name": "tablePredictions",
			"type": "Boolean"
		},
		{
			"name": "tableDistances",
			"type": "Boolean"
		},
		{
			"name": "tableWeights",
			"type": "Boolean"
		},
		{
			"name": "plotErrorVsK",
			"type": "Boolean"
		},
		{
			"name": "naAction",
			"type": "List",
			"options": [ "deleteListwise", "predict", "roughFix" ],
			"default": "deleteListwise"
		},
		{
			"name": "distanceParameter",
			"type": "List",
			"options": [ "auto", "manual", "optimized" ],
			"default": "auto"
		},
        {
            "name": "distanceParameterManual",
            "type": "Number",
            "value": 1,
            "min": 0.1
        },
		{
			"name": "scaleEqualSD",
			"type": "Boolean"
		},
		{
			"name": "validationLeaveOneOut",
			"type": "Boolean"
		},
		{
			"name": "maxK",
            "type": "Integer",
            "default": 5,
            "min": 1
		},
		{
			"name": "validationKFold",
			"type": "Boolean"
		},
		{
			"name": "noOfFolds",
            "type": "Integer",
            "default": 4,
            "min": 1
        },
        {
            "name": "weights",
            "type": "List",
            "options": ["unweighted", "optimal", "triangular", "epanechnikov" , "biweight", "triweight", "cos", "inv", "gaussian", "rank" ],
            "default": "unweighted"
        },
        {
            "name": "predictionsFrom",
            "type": "Integer",
            "min": 1,
            "max": 999999,
            "default": 1
        },
        {
            "name": "predictionsTo",
            "type": "Integer",
            "min": 1,
            "max": 999999,
            "default": 10
        }
	]
}
